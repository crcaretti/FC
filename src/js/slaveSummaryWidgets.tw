window.clearSummaryCache = function clearSummaryCache(slave) {
	if (!slave)
		setup.summaryCache = {};
	else if (slave instanceof Object && slave.ID !== Infinity && slave.ID !== -Infinity)
		setup.summaryCache[slave.ID] = undefined;
	else
		setup.summaryCache[slave] = undefined;
};
