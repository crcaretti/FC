:: Art_Vector_Revamp_Hair_Fore_ [nobr]

/*Default _hairArt value and fallback incase it is not set before calling include*/
<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Messy">>

<<if _showHair == 1>>
	<<switch _artSlave.hStyle>>
	<<case "neat">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Neat">>
	<<case "bun" "up">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Bun">>
	<<case "braided">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Braids">>
	<<case "tails">>
		/*No special art for fore tails use fore bun*/
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Bun">>
	<<case "ponytail">>
		/*No special art for fore ponytail use fore bun*/
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Bun">>
	<<case "curled">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Curled">>
	<<case "cornrows">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Cornrows">>
	<<case "strip">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Shaved_Sides">>
	<<case "luxurious">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Luxurious">>	
	<<case "afro">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Afro">>	
	<<case "permed">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Permed">>	
	<<case "dreadlocks">>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Dreadlocks">>	
	<<default>>
		<<set _hairForeArt = "Art_Vector_Revamp_Hair_Fore_Messy">>
	<</switch>>
	
	<<include _hairForeArt >>
<</if>>
