:: Art_Vector_Revamp_Head_ [nobr]


<<include Art_Vector_Revamp_Head>>

<<if _showEyes == 1>>
	<<if _artSlave.devotion > 50>>
		<<include Art_Vector_Revamp_Eyes_Happy>>
		<<if _showEyesHighlight == 1>>
			<<include Art_Vector_Revamp_Eyes_Happy_Highlights>>
		<</if>>
		<<if $showBodyMods>>
			<<if _artSlave.eyebrowPiercing == 1>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Happy_Piercing>>
			<<elseif _artSlave.eyebrowPiercing == 2>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Happy_Piercing_Heavy>>
			<</if>>
		<</if>>
	<<elseif _artSlave.devotion >= 0>>
		<<include Art_Vector_Revamp_Eyes_Shy>>
		<<if _showEyesHighlight == 1>>
			<<include Art_Vector_Revamp_Eyes_Shy_Highlights>>
		<</if>>
		<<if $showBodyMods>>
			<<if _artSlave.eyebrowPiercing == 1>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Shy_Piercing>>
			<<elseif _artSlave.eyebrowPiercing == 2>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Shy_Piercing_Heavy>>
			<</if>>
		<</if>>
	<<elseif _artSlave.devotion >= -50>>
		<<include Art_Vector_Revamp_Eyes_Closed>>
		<<if $showBodyMods>>
			<<if _artSlave.eyebrowPiercing == 1>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Closed_Piercing>>
			<<elseif _artSlave.eyebrowPiercing == 2>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Closed_Piercing_Heavy>>
			<</if>>
		<</if>>
	<<else>>
		<<include Art_Vector_Revamp_Eyes_Angry>>
		<<if _showEyesHighlight == 1>>
			<<include Art_Vector_Revamp_Eyes_Angry_Highlights>>
		<</if>>
		<<if $showBodyMods>>
			<<if _artSlave.eyebrowPiercing == 1>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Angry_Piercing>>
			<<elseif _artSlave.eyebrowPiercing == 2>>
				<<include Art_Vector_Revamp_Head_Eyebrow_Angry_Piercing_Heavy>>
			<</if>>
		<</if>>
	<</if>>
<</if>>

<<if _showMouth == 1>>
	<<if _artSlave.trust > 20>>
		<<if _artSlave.lips > 95>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Happy_Enormous>>
			<<include Art_Vector_Revamp_Mouth_Happy_Enormous>>
		<<elseif _artSlave.lips > 70>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Happy_Huge>>
			<<include Art_Vector_Revamp_Mouth_Happy_Huge>>
		<<elseif _artSlave.lips > 50>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Happy_Big>>
			<<include Art_Vector_Revamp_Mouth_Happy_Big>>
		<<elseif _artSlave.lips > 30>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Happy_Plush>>
			<<include Art_Vector_Revamp_Mouth_Happy_Plush>>
		<<elseif _artSlave.lips > 10>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Happy_Normal>>
			<<include Art_Vector_Revamp_Mouth_Happy_Normal>>
		<<else>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Happy_Thin>>
			<<include Art_Vector_Revamp_Mouth_Happy_Thin>>
		<</if>>
		<<if $showBodyMods>>
			<<if _artSlave.lipsPiercing == 1>>
				<<include Art_Vector_Revamp_Head_Mouth_Happy_Piercing>>
			<<elseif _artSlave.lipsPiercing == 2>>
				<<include Art_Vector_Revamp_Head_Mouth_Happy_Piercing_Heavy>>
			<</if>>
		<</if>>
	<<else>>
		<<if _artSlave.lips > 95>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Angry_Enormous>>
			<<include Art_Vector_Revamp_Mouth_Angry_Enormous>>
		<<elseif _artSlave.lips > 70>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Angry_Enormous>>
			<<include Art_Vector_Revamp_Mouth_Angry_Enormous>>
		<<elseif _artSlave.lips > 50>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Angry_Big>>
			<<include Art_Vector_Revamp_Mouth_Angry_Big>>
		<<elseif _artSlave.lips > 30>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Angry_Plush>>
			<<include Art_Vector_Revamp_Mouth_Angry_Plush>>
		<<elseif _artSlave.lips > 10>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Angry_Normal>>
			<<include Art_Vector_Revamp_Mouth_Angry_Normal>>
		<<else>>
			<<include Art_Vector_Revamp_Makeup_Mouth_Angry_Thin>>
			<<include Art_Vector_Revamp_Mouth_Angry_Thin>>
		<</if>>
		<<if $showBodyMods>>
			<<if _artSlave.lipsPiercing == 1>>
				<<include Art_Vector_Revamp_Head_Mouth_Angry_Piercing>>
			<<elseif _artSlave.lipsPiercing == 2>>
				<<include Art_Vector_Revamp_Head_Mouth_Angry_Piercing_Heavy>>
			<</if>>
		<</if>>
	<</if>>
<</if>>

<<if _showHeadHighlight == 1>>
	<<include Art_Vector_Revamp_Face_Highlights>>
<</if>>

/* ADDONS */
<<if $showBodyMods>>
	<<if _artSlave.earPiercing == 1>>
		<<include Art_Vector_Revamp_Head_Ear_Piercing>>
    <<elseif _artSlave.earPiercing == 2>>
		<<include Art_Vector_Revamp_Head_Ear_Piercing_Heavy>>
    <</if>>
	
	<<if _artSlave.nosePiercing == 1>>
		<<include Art_Vector_Revamp_Head_Nose_Piercing>>
    <<elseif _artSlave.nosePiercing == 2>>
		<<include Art_Vector_Revamp_Head_Nose_Piercing_Heavy>>
    <</if>>
<</if>>
<<switch _artSlave.collar>>
	<<case "dildo gag">>
		<<include Art_Vector_Revamp_Dildo_Gag>>
	<<case "bit gag">>
		<<include Art_Vector_Revamp_Bit_Gag>>
	<<case "ball gag">>
		<<include Art_Vector_Revamp_Ball_Gag>>
	<<default>>
	<</switch>>

<<if _artSlave.eyewear == "corrective glasses" or _artSlave.eyewear == "glasses" or _artSlave.eyewear == "blurring glasses">>
  <<include Art_Vector_Revamp_Glasses>>
<</if>>
