:: AS Dump

<<set $dumped = 0>>

<<for $i = 0; $i < $slaves.length; $i++>>
	<<if $activeSlave.ID == $slaves[$i].ID>>
	<<set $slaves[$i] = $activeSlave>>
	<<set $dumped = 1>>
	<<break>>
	<</if>>
<</for>>

<<if $dumped == 0>>
	<<if def $activeSlave.slaveName>>
	<<AddSlave $activeSlave>>
	<<set $dumped = 1>>
	<</if>>
<</if>>

<<goto $returnTo>>
