					<<include "Surgery Degradation">>
				<</if>>
			<<case "voicebox">>
				<<if ($activeSlave.voice == 0)>>
					<<set $cash -= $surgeryCost>>
					
					<<if ($activeSlave.ovaries + $activeSlave.hormones) > 1>>
