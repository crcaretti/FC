					<br><hr>
					<<include "Surgery Degradation">>
					
					<<set $organs.deleteAt(_o)>>
				<</if>>
			<<case "ovaries">>
				<<if ($activeSlave.ovaries == 0) && ($activeSlave.vagina >= 0) && ($activeSlave.mpreg == 0) && ($activeSlave.bellyImplant == 0)>>
