	<</if>>
	<<if $slaves[_i].relation == 0>>
		<<set $slaves[_i].relationTarget = 0>>
	<</if>>
	<<if $slaves[_i].relationship == 0>>
		<<set $slaves[_i].relationshipTarget = 0>>
	<</if>>
	<<if $slaves[_i].rivalry == 0>>
