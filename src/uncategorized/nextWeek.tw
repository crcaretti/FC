	<<assignJob $slaves[_OldHG] "live with your Head Girl">>
	<<set $HeadGirl = $slaves[_NewHG], $slaves[_NewHG].assignment = "be your Head Girl", $slaves[_NewHG].diet = "healthy">>
<</if>>

