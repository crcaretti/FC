/% Begin mixed race rate adjustment. %/
/% Some countries are extremely ethnically homogeneous and unlikely to change soon. %/
<<if ($args[0].nationality == "Japanese")>>
	<<if random(1,100) > 98>>
		<<set $args[0].race = "mixed race">>
	<</if>>
<<elseif ($args[0].nationality == "Korean")>>
	<<if random(1,100) > 95>>
		<<set $args[0].race = "mixed race">>
	<</if>>
<<elseif ($args[0].nationality == "Polish") || ($args[0].nationality == "Romanian") || ($args[0].nationality == "Bulgarian") || ($args[0].nationality == "Lithuanian") || ($args[0].nationality == "Croatian")>>
	/% Croatia isn't an origin currently but the game might add it in the future. %/
	<<if random(1,100) > 98>>
		<<set $args[0].race = "mixed race">>
	<</if>>
<<elseif random(1,100) > 90>>
	/% Default rate. %/
	<<set $args[0].race = "mixed race">>
<</if>>
/% End mixed race rate adjustment. %/

