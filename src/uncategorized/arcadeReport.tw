		<<elseif $slaves[$i].prostate == 1>>
			<<set $slaves[$i].prostate = 2, $slaves[$i].health -= 10, $cash -= $surgeryCost, $prostatesImplanted++>>
		<<elseif ($slaves[$i].lactation > 0) || ($slaves[$i].balls > 0)>>
			<<set _oldCash = $cash>>
			<<if $showEWD != 0>>
