:: SE Death [nobr] 
 
<<set $nextButton = "Continue", $returnTo = "Scheduled Event">>

<<for _sed = 0; _sed < $slaves.length; _sed++>>
<<if $slaves[_sed].death == "old">>
	<<DeathOldAge>>
	<<set $activeSlave = $slaves[_sed]>>
	<<include "Remove activeSlave">>
	<br><br><hr style="margin:0"><br>
<<elseif $slaves[_sed].death == "od">>
	<<DeathOverdose>>
	<<set $activeSlave = $slaves[_sed]>>
	<<include "Remove activeSlave">>
	<br><br><hr style="margin:0"><br>
<<elseif $slaves[_sed].death == "health">>
	<<DeathUnhealthy>>
	<<set $activeSlave = $slaves[_sed]>>
	<<include "Remove activeSlave">>
	<br><br><hr style="margin:0"><br>
<<elseif $slaves[_sed].death == "burst">>
	<<DeathPregnancyBurst>>
	<<set $activeSlave = $slaves[_sed]>>
	<<include "Remove activeSlave">>
	<br><br><hr style="margin:0"><br>
<</if>>

<</for>>
<<set $slaveDeath = 0>>

