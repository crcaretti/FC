			<</if>>
			<<if $Milkmaid == 0 && $dairy > 0>>
				<br>[[Milkmaid|Main][$JFCOrder = 1, $Role = "Milkmaid"]]
			<</if>>
		<</replace>>
		<</link>>
	<</if>>
	<<if ($DJ == 0 && $club > 0) || ($Madam == 0 && $brothel > 0) || ($Lurcher == 0 && $CoursingAssociation == 1) || ($Concubine == 0 && $masterSuite > 0)>>
		<br><<link "Entertain">>
